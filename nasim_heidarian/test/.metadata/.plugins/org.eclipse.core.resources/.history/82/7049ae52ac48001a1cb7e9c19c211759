package Car;

public abstract class car {
		
		public abstract float power();
		public abstract float fuel();
		public abstract float speed();
		

	}
abstract class BMW extends car{
	
	public float fuel() {
		return 12;
		
	}
}
class I8 extends BMW{
		
		public float power() {
			return 0;
		}
		
		public float speed() {
			return 0;
		}
}
  
abstract class AUDI extends car{
	
	public float power() {
		return 10;
	}
		
	}
	
}