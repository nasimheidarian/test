package Com.Revature.Models;

import java.io.Serializable;

public class Operation implements Serializable {
private static final long serialVersionUID = 1L;
	
	private int id;
	private Type type;
	private Account src;
	private Account dst;
	private float amount;
	
	public Operation() {
		super();
	}
	
	public Operation(int id) {
		super();
		this.id = id;
	}
	
	public Operation(int id, Type type, Account src, Account dst, float amount) {
		super();
	
		this.type = type;
		this.src = src;
		this.dst = dst;
		this.amount = amount;
	}
	

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Type gettype() {
		return type;
	}

	public void settype(Type type) {
		this.type =type;
	}

	public Account getsrc() {
		return src;
	}

	public void setsrc(Account src) {
		this.src = src;
	}
	
	public Account getdst() {
		return dst;
	}

	public void setdst(Account dst) {
		this.dst = dst;
	}

	public float getamount() {
		return amount;
	}

	public void setamount(float amount) {
		this.amount = amount;
	}
	public String getpassword() {
		return password;
	}

	public void password(String password) {
		this.password = password;
	}

	
	

}
